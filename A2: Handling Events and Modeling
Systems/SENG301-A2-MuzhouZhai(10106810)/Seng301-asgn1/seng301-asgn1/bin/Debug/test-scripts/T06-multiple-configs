CREATE(5, 10, 25, 200; 3)
CONFIGURE([0] "Coke", 20; "water", 250; "stuff", 200)
COIN_LOAD([0] 1; 10, 3)
COIN_LOAD([0] 0; 5, 3)
POP_LOAD([0] 2; "stuff", 2)
POP_LOAD([0] 1; "water", 1)
POP_LOAD([0] 0; "Coke", 1)
INSERT([0] 25)
PRESS([0] 0)
EXTRACT([0])
CHECK_DELIVERY(5, "Coke")
CONFIGURE([0] "AYY", 10; "BEE", 200; "CEE", 200)
POP_LOAD([0] 1; "BEE", 2)
INSERT([0] 200)
PRESS([0] 1)
EXTRACT([0])
CHECK_DELIVERY(0, "water")
UNLOAD([0])
CHECK_TEARDOWN(40; 225; "stuff", "stuff", "BEE", "BEE")