CREATE(5, 10, 25, 100; 2; 10; 10; 10)
CONFIGURE([0] "Coke", 250; "Sprite", 250)
CONFIGURE([0] "Water", 100; "Juice", 100)
COIN_LOAD([0] 2; 25, 10)
POP_LOAD([0] 0; "Water", 1)
INSERT([0] 100)
INSERT([0] 100)
INSERT([0] 25)
INSERT([0] 25)
PRESS([0] 0)
EXTRACT([0])
CHECK_DELIVERY(150, "Water")
UNLOAD([0])