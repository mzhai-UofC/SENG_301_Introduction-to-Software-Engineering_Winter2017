CREATE(5, 10, 25, 100; 2; 10; 10; 10)
CREATE(5, 10, 25, 100; 2; 10; 10; 10)
CONFIGURE([0] "Coke", 250; "Sprite", 250)
CONFIGURE([1] "Water", 100; "Juice", 100)
POP_LOAD([0] 0; "Coke", 1)
POP_LOAD([1] 0; "Water", 1)
INSERT([0] 100)
INSERT([0] 100)
INSERT([0] 25)
INSERT([0] 25)
PRESS([0] 0)
EXTRACT([0])
CHECK_DELIVERY(0, "Coke")
INSERT([1] 100)
PRESS([1] 0)
EXTRACT([1])
CHECK_DELIVERY(0, "Water")
UNLOAD([0])